page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

page {
    --themeColor: rgb(235, 68, 80);
    font-size: 28rpx;
}

image {
    width: 100%;
}

@font-face {
    font-family: "iconfont";
    src: url('//at.alicdn.com/t/font_1310192_5maozcqo369.eot?t=1582878904170');
    /* IE9 */
    src: url('//at.alicdn.com/t/font_1310192_5maozcqo369.eot?t=1582878904170#iefix') format('embedded-opentype'),
        /* IE6-IE8 */
        url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
        url('//at.alicdn.com/t/font_1310192_5maozcqo369.woff?t=1582878904170') format('woff'),
        url('//at.alicdn.com/t/font_1310192_5maozcqo369.ttf?t=1582878904170') format('truetype'),
        /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
        url('//at.alicdn.com/t/font_1310192_5maozcqo369.svg?t=1582878904170#iconfont') format('svg');
    /* iOS 4.1- */
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

